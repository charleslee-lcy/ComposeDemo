<?xml version="1.0" encoding="utf-8"?>
<resources>
    
    <declare-styleable name="FlipView">
        <attr name="orientation" format="enum">
            <enum name="vertical" value="0"/>
            <enum name="horizontal" value="1"/>
        </attr>
        <attr name="overFlipMode" format="enum">
            <enum name="glow" value="0"/>
            <enum name="rubber_band" value="1"/>
        </attr>
    </declare-styleable>

    <declare-styleable name="StatusBarHeightView">
        <attr name="use_type" format="integer">
            <enum name="use_height" value="0" />
            <enum name="use_padding_top" value="1" />
        </attr>
    </declare-styleable>
    
</resources>